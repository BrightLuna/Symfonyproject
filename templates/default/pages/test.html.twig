{% extends "base.html.twig" %}

{% block title %} {{ parent() }} - Test page{% endblock %}

{% block body %}

    <h1>Testing page</h1>
    
    {% include "partials/components/_discord_button.html.twig" with { discordLink: discordLink }%}
    

    <table class="table table-bordered mt-4">
    <thead>
        <tr>
        <th scope="col">Id</th>
        <th scope="col">Username</th>
        <th scope="col">Steam ID</th>
        </tr>
    </thead>
    <tbody>
        {% for user in users %}
        <tr>
        <th scope="row">{{ user.id }}</th>
        <td>{{ user.username }}</td>
        <td>{{ user.steamid }}</td>
        </tr>
        {% endfor %}
    </tbody>
    </table>

{% endblock %}
